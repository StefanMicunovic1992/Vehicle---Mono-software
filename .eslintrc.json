{
  "extends": ["next/core-web-vitals", "airbnb", "airbnb-typescript"],
  "plugins": ["jsx-a11y"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": "off",
    "max-len": "off",
    "import/no-extraneous-dependencies": "off",
    "react-hooks/exhaustive-deps": "off",
    "@typescript-eslint/no-unused-expressions": "off"
  }
}
